
public class Test {

	public static void main(String[] args) {
		
		A a = new A();
		a.m1(null);
		
	}
	
}

class A {
	
	public void m1(A a) {
		System.out.println("aaaa");
	}
	
	public void m1(B b) {
		System.out.println("bbbb");
	}
	
	public void m1(C c) {
		System.out.println("cccc");
	}
}

class B extends A {
	
}

class C extends A {
	
}